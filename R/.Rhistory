library( nls )
data(BOD)
formula(BOD)
fm1 <- nls(demand ~ SSasympOrig(Time, Asym, lrc), data = BOD)
pr1 <- profile(fm1)
library(splines)
sp1 <- interpSpline( tau ~ par.vals$Asym, pr1[[1]] )
pr1[[1]]
sp1 <- interpSpline( tau ~ par.vals[,"Asym"], pr1[[1]] )
sp1
plot(sp1)
str(interp.spline(pr1[[1]]$par.vals[,1], pr1[[1]]$tau))
str(interpSpline( tau ~ par.vals[,"Asym"], pr1[[1]], bSpline = TRUE )
)
nq()
